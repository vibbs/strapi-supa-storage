{
  "name": "strapi-supa-storage-provider",
  "version": "1.0.6",
  "description": "Supabase storage provider for Strapi v4 upload plugin",
  "homepage": "http://strapi.io",
  "keywords": [
    "upload",
    "supabase",
    "storage",
    "strapi",
    "provider"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib",
  "scripts": {
    "publish": "npm version patch --no-git-tag-version && npm publish",
    "publish-beta": "npm version pre --preid beta --no-git-tag-version && npm publish --tag beta"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.26.0"
  },
  "strapi": {
    "isProvider": true
  },
